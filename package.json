{
  "name": "vectra-crud",
  "version": "0.1.0",
  "description": "A flexible Vue 3 + TypeScript CRUD library with schema-driven components and pluggable UI adapters",
  "type": "module",
  "packageManager": "pnpm@8.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/vectra-crud.umd.cjs",
  "module": "./dist/vectra-crud.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vectra-crud.js",
      "require": "./dist/vectra-crud.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "element-plus": "^2.4.0"
  },
  "dependencies": {
    "vxe-table": "^4.5.0",
    "vxe-table-plugin-element": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.0",
    "element-plus": "^2.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.18.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "crud",
    "typescript",
    "element-plus",
    "schema-driven",
    "admin",
    "table",
    "form"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kowyzhux/vectra-crud.git"
  }
}
