{
  "name": "vectra-crud",
  "version": "0.1.0",
  "description": "Extensible Vue 3 + TypeScript CRUD library for ERP/enterprise back-office applications",
  "type": "module",
  "main": "./dist/vectra-crud.cjs",
  "module": "./dist/vectra-crud.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vectra-crud.js",
      "require": "./dist/vectra-crud.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "vue",
    "vue3",
    "crud",
    "typescript",
    "erp",
    "enterprise",
    "element-plus",
    "admin",
    "back-office"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0",
    "element-plus": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "element-plus": "^2.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  }
}
